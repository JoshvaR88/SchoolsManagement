<p id="notice"><%= notice %></p>

<h1>Enrolment Requests</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Batch</th>
      <th>Status</th>
      <th>Status</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrolment_requests.each do |enrolment_request| %>
      <tr>
        <td><%= enrolment_request.student_id %></td>
        <td><%= enrolment_request.batch_id %></td>
        <td><%= enrolment_request.status %></td>
        <td><%= enrolment_request.status %></td>
        <td><%= enrolment_request.status %></td>
        <td><%= link_to 'Show', enrolment_request %></td>
        <td><%= link_to 'Edit', edit_enrolment_request_path(enrolment_request) %></td>
        <td><%= link_to 'Destroy', enrolment_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

